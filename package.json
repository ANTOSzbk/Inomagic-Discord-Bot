  "repository": {
    "type": "git",
    "url": "git+https://github.com/ANTOSzbk/peanut.git"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "multi",
    "purpose",
    "gaming",
    "integration"
  ],
  "author": "ANTOSzbk <adi@antoszbk.xyz>",
  "license": "MIT",
  "dependencies": {
    "@naval-base/ms": "^3.1.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.17",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "common-tags": "^1.8.0",
    "discord-akairo": "^8.0.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "node-fetch": "^2.6.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.13.5",
