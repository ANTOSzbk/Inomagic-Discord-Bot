{
  "name": "peanut",
  "version": "0.0.1",
  "description": "Multi-purpose Discord Bot.",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "develop": "nodemon",
    "start": "node build/app.js",
    "lint": "npx eslint src/* --ext .ts",
    "lint:fix": "npx eslint src/* --ext .ts --fix",
